{
  "name": "company-employee-api",
  "version": "0.0.1",
  "description": "Company/Employee management Restful API",
  "scripts": {
    "eslint": "eslint '**/*.ts'",
    "prettier": "prettier '**/*.ts'",
    "eslint:fix": "eslint --fix '**/*.ts'",
    "prettier:fix": "prettier --write '**/*.ts'",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coverage": "npm test -- --coverage",
    "test:unit": "npm test -- -c jestUnit.config.js",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "author": "devdbreno <dbrenoti@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/devdbreno/employee-management-api.git",
  "dependencies": {
    "module-alias": "^2.2.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/module-alias": "^2.0.1",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "coveralls": "^3.1.1",
    "eslint": "^7.30.0",
    "faker-br": "^0.4.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "npm": ">=6.x",
    "node": ">=12.x"
  }
}
